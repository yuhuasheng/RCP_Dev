      <handler class="com.foxconn.electronics.login.MyIssuesListRefresh" commandId="org.eclipse.ui.file.refresh">
        <activeWhen>
                <and>
                    <iterate ifEmpty="false">
                    	<adapt type="com.teamcenter.rac.kernel.TCComponent">
                			<and>
               					<or>
               						<test property="com.teamcenter.rac.kernel.TCComponent.typeClass"  value="D9_IssueHome" />
               						<test property="com.teamcenter.rac.kernel.TCComponent.typeClass"  value="D9_IssueSearch" />
               					</or>
               						<!-- <test forcePluginActivation="true" property="ghy.dba"/> -->
               				</and>
            			</adapt>
                    </iterate>
                    <with variable="rac_command_suppression">
                        <not>
                            <iterate operator="or">
                                <equals value="org.eclipse.ui.file.refresh"/>
                            </iterate>
                        </not>
                    </with>
                </and>
            </activeWhen>
      </handler>
      
      <handler class="com.foxconn.electronics.login.MyIssuesListCreateProcess" commandId="com.teamcenter.rac.newProcess">
      		<activeWhen>
                <and>
                    <iterate ifEmpty="false">
                    	<adapt type="com.teamcenter.rac.kernel.TCComponent">
                			<and>
               					<test property="com.teamcenter.rac.kernel.TCComponent.typeClass"  value="IssueReportRevision" />
               				</and>
            			</adapt>
                    </iterate>
                    <with variable="rac_command_suppression">
                        <not>
                            <iterate operator="or">
                                <equals value="com.teamcenter.rac.newProcess"/>
                            </iterate>
                        </not>
                    </with>
                </and>
            </activeWhen>
      </handler>
