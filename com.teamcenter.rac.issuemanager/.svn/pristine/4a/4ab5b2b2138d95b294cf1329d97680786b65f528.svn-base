    <extension point="org.eclipse.ui.handlers">
        <handler
            class="com.teamcenter.rac.issuemanager.handlers.issuehome.ManageSavedSearchesHandler"
            commandId="com.teamcenter.rac.issuemanager.issuehome.manageSavedSearches">
        </handler>
        <handler
            class="com.teamcenter.rac.issuemanager.handlers.issuehome.SearchDeleteHandler"
            commandId="org.eclipse.ui.edit.delete">
            <activeWhen>
                <and>
                    <iterate ifEmpty="false">
                        <instanceof
                            value="com.teamcenter.rac.issuemanager.ui.issuehome.AbstractSearchPseudoFolder">
                        </instanceof>
                    </iterate>
                    <with variable="rac_command_suppression">
                        <not>
                            <iterate operator="or">
                                <equals value="org.eclipse.ui.edit.delete"/>
                            </iterate>
                        </not>
                    </with>
                </and>
            </activeWhen>
            <enabledWhen>
                <!--
                Issue list folder delete enabled when one or more folders which the 
                user has permission to delete are selected
                -->
                <iterate ifEmpty="false">
                    <or>
                        <and>
