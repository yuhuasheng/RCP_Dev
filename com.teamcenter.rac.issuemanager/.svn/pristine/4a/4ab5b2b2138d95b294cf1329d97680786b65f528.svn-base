<plugin>
    <extension point="org.eclipse.ui.perspectives">
        <perspective
            class="com.teamcenter.rac.aifrcp.perspective.GenericRACPerspective"
            icon="icons/issue_manager_16.png"
            id="com.teamcenter.rac.issuemanager.perspectives.issueManager"
            name="%im.issueManager.PERSPECTIVE">
        </perspective>
    </extension>
    <extension point="org.eclipse.ui.perspectiveExtensions">
        <perspectiveExtension
            targetID="com.teamcenter.rac.issuemanager.perspectives.issueManager">
            <viewShortcut id="com.teamcenter.rac.issuemanager.views.issueHome"/>
            <viewShortcut id="com.teamcenter.rac.issuemanager.views.issuePreview"/>
            <viewShortcut id="com.teamcenter.rac.issuemanager.views.listDetails"/>
        </perspectiveExtension>
    </extension>
    <extension point="org.eclipse.ui.views">
        <category id="com.teamcenter.rac.issuemanager.views"
            name="%im.viewCategory">
        </category>
