                icon="icons/issue_savedsearch_16.png"
                label="%im.issuefolder.manage.LABEL"
                tooltip="%im.issuefolder.manage.TOOLTIP">
                <visibleWhen>
                    <with variable="rac_command_suppression">
                        <not>
                            <iterate operator="or">
                                <equals
                                    value="com.teamcenter.rac.issuemanager.issuehome.manageSavedSearches"/>
                            </iterate>
                        </not>
                    </with>
                </visibleWhen>
            </command>
            <separator name="managesearches" visible="true">
            </separator>
            <separator name="additions">
            </separator>
        </menuContribution>
        <menuContribution
            locationURI="toolbar:com.teamcenter.rac.issuemanager.views.issueHome?after=additions">
            <command
                commandId="com.teamcenter.rac.issuemanager.issuehome.manageSavedSearches"
                icon="icons/issue_savedsearch_16.png"
                label="%im.issuefolder.manage.LABEL"
                tooltip="%im.issuefolder.manage.TOOLTIP">
                <visibleWhen>
                    <with variable="rac_command_suppression">
                        <not>
                            <iterate operator="or">
                                <equals
                                    value="com.teamcenter.rac.issuemanager.issuehome.manageSavedSearches"/>
                            </iterate>
                        </not>
                    </with>
                </visibleWhen>
            </command>
        </menuContribution>
        <menuContribution
            locationURI="popup:com.teamcenter.rac.issuemanager.views.issueHome?after=additions">
            <command commandId="org.eclipse.ui.file.refresh">
                <visibleWhen>
                    <and>
                        <iterate ifEmpty="false">
                            <or>
                                <adapt
                                    type="com.teamcenter.rac.issuemanager.ui.issuehome.IssueHomePseudoFolder">
                                </adapt>
                                <adapt
                                    type="com.teamcenter.rac.issuemanager.ui.issuehome.IssueHomeQueryPseudoFolder">
                                </adapt>
                                <adapt
                                    type="com.teamcenter.rac.issuemanager.ui.issuehome.IssueHomeSearchPseudoFolder">
                                </adapt>
                            </or>
                        </iterate>
                        <with variable="rac_command_suppression">
                            <not>
                                <iterate operator="or">
                                    <equals value="org.eclipse.ui.file.refresh"/>
                                </iterate>
                            </not>
                        </with>
                        <with variable="rac_contextmenu_suppression">
                            <not>
                                <iterate operator="or">
                                    <equals value="org.eclipse.ui.file.refresh">
                                    </equals>
                                </iterate>
                            </not>
                        </with>
                    </and>
                </visibleWhen>
            </command>
            <command commandId="org.eclipse.ui.edit.delete">
                <visibleWhen>
                    <and>
                        <iterate ifEmpty="false">
                            <or>
                                <adapt
                                    type="com.teamcenter.rac.issuemanager.ui.issuehome.IssueHomeQueryPseudoFolder">
                                </adapt>
                                <adapt
                                    type="com.teamcenter.rac.issuemanager.ui.issuehome.IssueHomeSearchPseudoFolder">
                                </adapt>
                            </or>
                        </iterate>
                        <with variable="rac_command_suppression">
                            <not>
                                <iterate operator="or">
                                    <equals value="org.eclipse.ui.edit.delete"/>
                                </iterate>
                            </not>
                        </with>
                        <with variable="rac_contextmenu_suppression">
                            <not>
                                <iterate operator="or">
                                    <equals value="org.eclipse.ui.edit.delete">
                                    </equals>
                                </iterate>
                            </not>
                        </with>
                    </and>
                </visibleWhen>
            </command>
            <separator name="folderoptions" visible="true">
            </separator>
        </menuContribution>
        <menuContribution
            locationURI="popup:com.teamcenter.rac.sendto?after=additions">
            <command
                commandId="com.teamcenter.rac.issuemanager.sendToIssueManager"
                icon="icons/issue_manager_16.png">
                <visibleWhen>
                    <and>
                        <iterate ifEmpty="false">
                            <adapt type="com.teamcenter.rac.kernel.TCComponent">
                                <or>
                                    <test
                                        property="com.teamcenter.rac.kernel.TCComponent.typeClass"
                                        value="IssueReport,IssueReportRevision,">
                                    </test>
                                </or>
                            </adapt>
                        </iterate>
                        <with variable="rac_command_suppression">
                            <not>
                                <iterate operator="or">
                                    <equals
                                        value="com.teamcenter.rac.issuemanager.sendToIssueManager"/>
                                </iterate>
                            </not>
                        </with>
                        <with variable="rac_contextmenu_suppression">
                            <not>
                                <iterate operator="or">
                                    <equals value="com.teamcenter.rac.issuemanager.sendToIssueManager">
                                    </equals>
                                </iterate>
                            </not>
                        </with>
                    </and>
                </visibleWhen>
            </command>
        </menuContribution>
        
        
       <menuContribution locationURI="popup:org.eclipse.ui.popup.any?after=additions">
			<command commandId="com.teamcenter.rac.issuemanager.reviewIssue">
		        <visibleWhen>
		              	<and>
							<with variable="rac_command_suppression">
		                     	<not>
		                        	<iterate operator="or">
		                           		<equals
		                                 	value="com.teamcenter.rac.issuemanager.reviewIssue">
		                           	</equals>
		                        </iterate>
		                      </not>
		                  	</with>
		                  	
		                  	<iterate ifEmpty="false">
		            			<adapt type="com.teamcenter.rac.kernel.TCComponent">
		                			<and>
		               					<test property="com.teamcenter.rac.kernel.TCComponent.typeClass"  value="IssueReportRevision" />		               					
		               				</and>
		            			</adapt>
		     				</iterate>
		     				
		     			</and>
				</visibleWhen>
		    </command>
    	</menuContribution>
    
        <menuContribution 
            locationURI="popup:org.eclipse.ui.popup.any?after=group.sendto">
            <command commandId="com.teamcenter.rac.issuemanager.issueEditCommandId" >  
                <visibleWhen>
                    <and>
                        <iterate ifEmpty="false">
                            <adapt type="com.teamcenter.rac.kernel.TCComponent">
                                <or>
                                    <test
                                        property="com.teamcenter.rac.kernel.TCComponent.typeClass"
                                        value="IssueReport,IssueReportRevision,">
                                    </test>
                                </or>
                            </adapt>
                        </iterate>
                        <with variable="rac_command_suppression">
                            <not>
                                <iterate operator="or">
                                    <equals 
                                        value="com.teamcenter.rac.issuemanager.issueEditCommandId"/>
                                </iterate>
                            </not>
                        </with>
                        <with variable="rac_contextmenu_suppression">
                            <not>
                                <iterate operator="or">
                                    <equals value="com.teamcenter.rac.issuemanager.issueEditCommandId">
                                    </equals>
                                </iterate>
                            </not>
                        </with>
                    </and>
                </visibleWhen>
            </command>
        </menuContribution>
        <menuContribution
              locationURI="menu:com.teamcenter.rac.issuemanager.views.listDetails?after=additions">
           <command
                 commandId="com.teamcenter.rac.issuemanager.issuehome.configViewCommand"
                 icon="platform:/plugin/com.teamcenter.rac.viewer/icons/table_view_16.png"
