            class="com.teamcenter.rac.issuemanager.views.IssueHome"
            icon="icons/issue_home_16.png"
            id="com.teamcenter.rac.issuemanager.views.issueHome"
            name="%im.issueHome.VIEW"/>
        <view allowMultiple="true"
            category="com.teamcenter.rac.issuemanager.views"
            class="com.teamcenter.rac.issuemanager.views.IssuePreview"
            icon="icons/issue_preview_16.png"
            id="com.teamcenter.rac.issuemanager.views.issuePreview"
            name="%im.issuePreview.VIEW" restorable="true"/>
        <view allowMultiple="true"
            category="com.teamcenter.rac.issuemanager.views"
            class="com.teamcenter.rac.issuemanager.views.ListDetails"
