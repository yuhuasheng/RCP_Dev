             id="com.teamcenter.rac.issuemanager.views.issueHome"
             selectionInPrimaryEffectsInvisbleSecondary="false"
             selectionInSecondaryEffectsPrimary="true">
       </view>
    </extension>
    <extension
          point="com.teamcenter.rac.aifrcp.perspectiveDefs">
       <perspective
             displayMode="Tertiary"
             icon16="icons/issue_manager_16.png"
             icon24="icons/issue_manager_24.png"
             icon32="icons/issue_manager_32.png"
             id="com.teamcenter.rac.issuemanager.perspectives.issueManager"
             isLegacyAppDefault="true"
             label="%im.issueManager.PERSPECTIVE"
             legacyAppId="com.teamcenter.rac.issuemanager.perspectives.issueManager"
             ordinality="0"
             tooltip="%im.issueManager.PERSPECTIVE.TIP">
          <contextRef
                id="com.teamcenter.rac.issuemanager.perspectives.issueManagerContext">
          </contextRef>
          <viewRef
                id="com.teamcenter.rac.issuemanager.views.issueHome">
          </viewRef>
          <viewRef
                id="com.teamcenter.rac.ui.views.SummaryView">
          </viewRef>
          <viewRef
                id="com.teamcenter.rac.ui.views.DetailsView">
          </viewRef>
          <viewRef
                id="com.teamcenter.rac.ui.views.ViewerView">
          </viewRef>
          <viewRef
                id="com.teamcenter.rac.ui.views.ImpactAnalysisView">
          </viewRef>
          <viewRef
                id="com.teamcenter.rac.issuemanager.views.issuePreview">
          </viewRef>
          <viewRef
                id="com.teamcenter.rac.issuemanager.views.listDetails">
          </viewRef>
       </perspective>
    </extension>
    <extension
          point="org.eclipse.ui.contexts">
       <context
             description="%im.issueManager.PERSPECTIVE"
             id="com.teamcenter.rac.issuemanager.perspectives.issueManagerContext"
             name="%im.issueManager.PERSPECTIVE"
             parentId="com.teamcenter.rac.common.TeamcenterContext">
       </context>
    </extension> 
    <extension point="com.teamcenter.rac.aifrcp.openWithViewMenuDef">
        <menuItem viewId="com.teamcenter.rac.issuemanager.views.issuePreview" label="%im.issuePreview.VIEW" 
            icon="icons/issue_preview_16.png" groupId="secondary" ordinal="200">
            <visibleWhen>
                <and>
                    <count value="1"/>
                    <iterate ifEmpty="false" operator="and">
                        <adapt type="com.teamcenter.rac.kernel.TCComponentIssueReportRevision"/>
                    </iterate>
                </and>
            </visibleWhen>
        </menuItem>
        <menuItem viewId="com.teamcenter.rac.issuemanager.views.listDetails" label="%im.listDetails.VIEW" 
            icon="platform:/plugin/com.teamcenter.rac.common/icons/properties_16.png" groupId="secondary" ordinal="200">
            <visibleWhen>
                <and>
                    <count value="1"/>
                    <iterate ifEmpty="false" operator="and">
                        <adapt type="com.teamcenter.rac.issuemanager.ui.issuehome.AbstractSearchPseudoFolder"/>
                    </iterate>
                </and>
            </visibleWhen>
        </menuItem>
    </extension>
    <extension point="org.eclipse.core.expressions.definitions">
        <definition id="com.teamcenter.rac.issuemanager.inIssuePerspective">
            <or>
                <with variable="activePartId">
                    <equals
                        value="com.teamcenter.rac.issuemanager.views.issueHome">
                    </equals>
                </with>
                <with variable="rac_property.ACTIVE_APPLICATION">
                    <equals
                        value="com.teamcenter.rac.issuemanager.perspectives.issueManager">
                    </equals>
                </with>
            </or>
        </definition>
        <definition id="com.teamcenter.rac.issuemanager.itemRevisionNotChange">
            <adapt type="com.teamcenter.rac.kernel.TCComponentItemRevision">
                <and>
                    <not>
                        <adapt 
                            type="com.teamcenter.rac.kernel.TCComponentChangeItemRevision"/>
                    </not>
                </and>
            </adapt>
        </definition>        
    </extension>
    <!-- BEGIN New Issue wizard -->
