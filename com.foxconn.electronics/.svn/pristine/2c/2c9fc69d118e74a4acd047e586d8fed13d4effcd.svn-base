package com.foxconn.electronics.explodebom.domain;

public class InputInfoBean
{
    // pcbea item id
    private String pcbNumber;
    // BOM栏位序号
    private int    bomNo;
    // phase枚举值 (MP or NPI )
    private String phase;

    public String getPcbNumber()
    {
        return pcbNumber;
    }

    public void setPcbNumber(String pcbNumber)
    {
        this.pcbNumber = pcbNumber;
    }

    public int getBomNo()
    {
        return bomNo;
    }

    public void setBomNo(int bomNo)
    {
        this.bomNo = bomNo;
    }

    public String getPhase()
    {
        return phase;
    }

    public void setPhase(String phase)
    {
        this.phase = phase;
    }
}
