                  <command
                        commandId="com.foxconn.mechanism.SecondSourceImport.ImportHandler"
                        id="com.foxconn.mechanism.SecondSourceImport.ImportHandler">
                        <visibleWhen>
                        	<and>
                            	<reference definitionId="com.teamcenter.rac.ui.inMainPerspective"/>
                            	<iterate ifEmpty="false">
									<adapt type="com.teamcenter.rac.kernel.TCComponent">
										<and>
											<test 
												forcePluginActivation="true"
												property="ghy.Pool_CEandMonitor_CEandPrinter_CE">
											</test>
										</and>
									</adapt>
								</iterate>
                            <with variable="rac_command_suppression">
                                <not>
                                    <iterate operator="or">
                                        <equals value="com.foxconn.mechanism.SecondSourceImport.ImportHandler"/>
                                    </iterate>
                                </not>
                            </with>
                        </and>
                    </visibleWhen>
                </command>
