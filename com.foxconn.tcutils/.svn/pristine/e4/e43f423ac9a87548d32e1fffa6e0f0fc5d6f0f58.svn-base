package com.foxconn.tcutils.constant;

import org.eclipse.swt.graphics.Color;

public enum ColorEnum {

	GoldGray(217, 217, 217), Gray(242, 242, 242), Blue(217, 225, 242), Green(226, 239, 218), Red(255, 204, 204), TipColor(255, 0, 0), HighLightColor(255, 224, 102);	
	private final Color color;
	
	private ColorEnum(int red, int green, int blue) {
    	this.color = new Color(null, red, green, blue); 
    }
	
	public Color color() {
    	return color;
	}
}
